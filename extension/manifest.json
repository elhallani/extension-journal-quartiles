{
  "manifest_version": 3,
  "name": "Journal Quartile Badge",
  "version": "1.0",
  "description": "Displays SJR quartile for journals on Google Scholar and Semantic Scholar.",
  "content_scripts": [
    {
      "matches": [
        "https://scholar.google.com/*",
        "https://www.semanticscholar.org/*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
